{
  "plugin": {
    "name": "Plugin",
    "sort": 100,
    "compilation": "default",
    "setup": {
      "base": "setups/${type.id}",
      "metaReplace": {
        "regex": "\\$\\{([^}]+)}",
        "files": [
          "LICENSE",
          "README.md",
          "package.json",
          "plugin.json"
        ],
        "paths": ["**/*"]
      },
      "meta": {
        "Name": "Plugin",
        "name": "plugin",
        "path": "plugins"
      }
    }
  }
}