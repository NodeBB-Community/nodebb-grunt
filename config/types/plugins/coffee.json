{
  "plugin-coffee": {
    "name": "Plugin with CoffeeScript compilation",
    "sort": 150,
    "compilation": "coffee",
    "setup": {
      "base": "setups/${type.id}",
      "metaReplace": {
        "regex": "\\$\\{([^}]+)}",
        "files": [
          "LICENSE",
          "README.md",
          "package.json",
          "plugin.json",
          "templates/**/*"
        ],
        "paths": ["**/*"]
      },
      "meta": {
        "Name": "CoffeeScript Plugin",
        "name": "plugin",
        "path": "plugins"
      }
    }
  }
}