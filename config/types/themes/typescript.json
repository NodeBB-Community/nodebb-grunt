{
  "theme-typescript": {
    "name": "Theme with TypeScript compilation",
    "sort": 1125,
    "compilation": "typescript",
    "setup": {
      "base": "setups/${type.id}",
      "metaReplace": {
        "regex": "\\$\\{([^}]+)}",
        "files": [
          "LICENSE",
          "README.md",
          "package.json",
          "theme.json",
          "plugin.json"
        ],
        "paths": ["**/*"]
      },
      "meta": {
        "Name": "TypeScript Theme",
        "name": "theme",
        "path": "themes"
      }
    }
  }
}