{
  "steps": {
    "meta": {
      "default": {
        "compiler": "moduleMeta",
        "src": [
          "package.json",
          "plugin.json",
          "theme.json",
          "lib/settings.*",
          "public/**/*",
          "!public/**/vendor/**/*"
        ],
        "regex": "@\\{([^}]+)}"
      }
    }
  }
}
