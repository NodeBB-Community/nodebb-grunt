{
  "plugin": {
    "name": "Plugin",
    "setup": {
      "base": "setups/${type.id}",
      "metaReplace": {
        "regex": "\\$\\{([^}]+)}",
        "files": [
          "LICENSE",
          "README.md",
          "package.json",
          "plugin.json"
        ],
        "paths": ["**/*"]
      }
    },
    "compilation": "default",
    "meta": {
      "Name": "Plugin",
      "name": "plugin",
      "path": "plugins"
    }
  },
  "plugin-coffee": {
    "name": "Plugin with CoffeeScript compilation",
    "setup": {
      "base": "setups/${type.id}",
      "metaReplace": {
        "regex": "\\$\\{([^}]+)}",
        "files": [
          "LICENSE",
          "README.md",
          "package.json",
          "plugin.json",
          "templates/**/*"
        ],
        "paths": ["**/*"]
      }
    },
    "compilation": "coffee",
    "meta": {
      "Name": "CoffeeScript Plugin",
      "name": "plugin",
      "path": "plugins"
    }
  },
  "theme": {
    "name": "Theme",
    "setup": {
      "base": "setups/${type.id}",
      "metaReplace": {
        "regex": "\\$\\{([^}]+)}",
        "files": [
          "LICENSE",
          "README.md",
          "package.json",
          "theme.json",
          "plugin.json"
        ],
        "paths": ["**/*"]
      }
    },
    "compilation": "default",
    "meta": {
      "Name": "Theme",
      "name": "theme",
      "path": "themes"
    }
  },
  "theme-coffee": {
    "name": "Theme with CoffeeScript compilation",
    "setup": {
      "base": "setups/${type.id}",
      "metaReplace": {
        "regex": "\\$\\{([^}]+)}",
        "files": [
          "LICENSE",
          "README.md",
          "package.json",
          "theme.json",
          "plugin.json"
        ],
        "paths": ["**/*"]
      }
    },
    "compilation": "coffee",
    "meta": {
      "Name": "CoffeeScript Theme",
      "name": "theme",
      "path": "themes"
    }
  }
}