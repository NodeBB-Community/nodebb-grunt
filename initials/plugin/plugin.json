{
  "id": "nodebb-${type}-${id}",
  "name": "${name}",
  "description": "${description}",
  "library": "@{main}",
  "staticDirs": {},
  "less": [],
  "languages": "locales",
  "templates": "templates",
  "hooks": [
    {
      "hook": "action:plugin.activate",
      "method": "pluginActivation"
    },
    {
      "hook": "filter:admin.header.build",
      "method": "addAdminNavigation"
    },
    {
      "hook": "action:app.load",
      "method": "init"
    }
  ],
  "nbbpm": {
    "compatibility": "^0.7.0"
  }
}
